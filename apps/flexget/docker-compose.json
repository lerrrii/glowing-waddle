{
  "$schema": "../dynamic-compose-schema.json",
  "services": [
    {
      "image": "lscr.io/linuxserver/flexget:latest",
      "name": "flexget",
      "internalPort": 5050,
      "isMain": true,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/config"
        },
        {
          "hostPath": "${APP_DATA_DIR}/data/flexget",
          "containerPath": "/data"
        }
      ],
      "environment": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "${TZ}",
        "FG_WEBUI_PASSWORD": "${FG_WEBUI_PASSWORD}"
      }
    }
  ]
}