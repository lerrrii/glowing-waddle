  "port": 9117,
  "id": "jackett-vpn",
  "tipi_version": 82,
  "version": "latest",
  "description": "Jackett running with WireGuard VPN protection and kernel-level kill-switch. Works with ProtonVPN and other WireGuard providers.",
  "short_desc": "Torrent indexer with VPN protection",
  "categories": ["media", "utilities", "security"],
  "author": "dyonr",
