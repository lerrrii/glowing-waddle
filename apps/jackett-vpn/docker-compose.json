{
  "services": [
    {
      "name": "gluetun",
      "image": "qmcgaw/gluetun:latest",
      "isMain": true,
      "internalPort": 9117,
      "capAdd": ["NET_ADMIN"],
      "devices": ["/dev/net/tun"],
      "environment": {
        "VPN_SERVICE_PROVIDER": "${JACKETT_VPN_SERVICE_PROVIDER}",
        "VPN_TYPE": "${JACKETT_VPN_TYPE}",
        "WIREGUARD_PRIVATE_KEY": "${JACKETT_WIREGUARD_PRIVATE_KEY}",
        "OPENVPN_USER": "${JACKETT_OPENVPN_USERNAME}",
        "OPENVPN_PASSWORD": "${JACKETT_OPENVPN_PASSWORD}",
        "SERVER_COUNTRIES": "${JACKETT_SERVER_COUNTRIES-Netherlands,United States,Japan}",
        "FREE_ONLY": "${JACKETT_FREE_ONLY-false}",
        "FIREWALL": "${JACKETT_FIREWALL-true}",
        "FIREWALL_VPN_INPUT_PORTS": "9117",
        "FIREWALL_OUTBOUND_SUBNETS": "",
        "DNS": "${JACKETT_DNS-true}",
        "DOT": "true",
        "DOT_FILTERING_BLOCK_ADS": "${JACKETT_BLOCK_ADS-false}",
        "DOT_FILTERING_BLOCK_SURVEILLANCE": "${JACKETT_BLOCK_SURVEILLANCE-false}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/gluetun",
          "containerPath": "/gluetun"
        }
      ],
      "logging": {
        "driver": "json-file",
        "options": {
          "max-size": "10m"
        }
      }
    },
    {
      "name": "jackett",
      "image": "linuxserver/jackett:latest",
      "networkMode": "service:gluetun",
      "environment": {
        "PUID": "${JACKETT_PUID-1000}",
        "PGID": "${JACKETT_PGID-1000}",
        "TZ": "${TZ-Europe/London}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/config"
        },
        {
          "hostPath": "${APP_DATA_DIR}/data/downloads",
          "containerPath": "/downloads"
        }
      ],
      "dependsOn": ["gluetun"]
    }
  ]
}
