      "hint": "Prowlarr search query timeout in seconds."
    },
    {
      "type": "number",
      "label": "Prowlarr Search Interval (Hours)",
      "env_variable": "PROWLARR_SEARCH_INTERVAL_HOUR",
      "default": 72,
      "placeholder": "72",
      "hint": "Prowlarr search interval in hours."
    },
    {
      "type": "number",
      "label": "Prowlarr Max Immediate Processes",
      "env_variable": "PROWLARR_IMMEDIATE_MAX_PROCESS",
