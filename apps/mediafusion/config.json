    {
      "type": "boolean",
      "label": "Prowlarr Background Title Search",
      "env_variable": "PROWLARR_BACKGROUND_TITLE_SEARCH",
      "default": true,
      "hint": "Enable background title search in Prowlarr."
    },
    {
      "type": "number",
      "label": "Prowlarr Search Query Timeout",
      "env_variable": "PROWLARR_SEARCH_QUERY_TIMEOUT",
