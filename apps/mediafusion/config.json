{
  "$schema": "../app-info-schema.json",
  "id": "mediafusion",
  "name": "MediaFusion+",
  "version": "latest",
  "tipi_version": 17,
  "available": true,
  "exposable": true,
  "dynamic_config": true,
  "port": 8000,
  "categories": [
    "media"
  ],
  "author": "mhdzumair + Runtipi community",
  "description": "Advanced Stremio addon with torrent streaming, debrid integration, and comprehensive scraping from multiple sources including Prowlarr, Jackett, and public trackers.",
  "short_desc": "Stremio addon with advanced torrent streaming and multi-source scraping.",
  "source": "https://github.com/mhdzumair/MediaFusion",
  "form_fields": [
    {
      "type": "text",
      "label": "Addon Name",
      "env_variable": "ADDON_NAME",
      "default": "MediaFusion",
      "placeholder": "MediaFusion",
      "hint": "Display name of the addon in Stremio."
    },
    {
      "type": "text",
      "label": "Host URL",
      "env_variable": "HOST_URL",
      "required": true,
      "placeholder": "https://mediafusion.yourdomain.com",
      "hint": "The URL where MediaFusion is hosted (required)."
    },
    {
      "type": "random",
      "label": "Secret Key",
      "min": 32,
      "env_variable": "SECRET_KEY",
      "required": true,
      "hint": "32-character secret key for session signing (will be auto-generated if not provided)."
    },
    {
      "type": "password",
      "label": "API Password",
      "env_variable": "API_PASSWORD",
      "required": true,
      "hint": "Password for accessing API endpoints and admin features."
    },
    {
      "type": "text",
      "label": "Contact Email",
      "env_variable": "CONTACT_EMAIL",
      "default": "admin@mediafusion.local",
      "placeholder": "admin@mediafusion.local",
      "hint": "Contact email for the MediaFusion addon."
    },
    {
      "type": "text",
      "label": "Description",
      "env_variable": "DESCRIPTION",
      "placeholder": "Advanced Stremio addon with torrent streaming",
      "hint": "Brief description shown on Stremio addon page."
    },
    {
      "type": "text",
      "label": "Timezone",
      "env_variable": "TZ",
      "default": "UTC",
      "placeholder": "UTC",
      "hint": "Timezone for the application (e.g., Europe/London, America/New_York)."
    },
    {
      "type": "text",
      "label": "Logging Level",
      "env_variable": "LOGGING_LEVEL",
      "default": "INFO",
      "placeholder": "INFO",
      "hint": "Application logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL."
    },
    {
      "type": "boolean",
      "label": "Public Instance",
      "env_variable": "IS_PUBLIC_INSTANCE",
      "default": false,
      "hint": "Enable for community instances (no auth except /scraper endpoint)."
    },
    {
      "type": "text",
      "label": "MongoDB URI",
      "env_variable": "MONGO_URI",
      "default": "mongodb://mongodb:27017/mediafusion",
      "placeholder": "mongodb://mongodb:27017/mediafusion",
      "hint": "MongoDB connection URI."
    },
    {
      "type": "number",
      "label": "Max Database Connections",
      "env_variable": "DB_MAX_CONNECTIONS",
      "default": 50,
      "placeholder": "50",
      "hint": "Maximum database connections."
    },
    {
      "type": "text",
      "label": "Redis URL",
      "env_variable": "REDIS_URL",
      "default": "redis://redis:6379",
