    {
      "type": "number",
      "label": "Prowlarr Max Process Time",
      "env_variable": "PROWLARR_IMMEDIATE_MAX_PROCESS_TIME",
      "default": 15,
      "placeholder": "15",
      "hint": "Maximum process time for Prowlarr in seconds."
