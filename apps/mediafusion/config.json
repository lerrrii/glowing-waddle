    {
      "type": "number",
      "label": "Max Redis Connections",
      "env_variable": "REDIS_MAX_CONNECTIONS",
      "default": 100,
      "placeholder": "100",
      "hint": "Maximum Redis connections."
    },
    {
      "type": "text",
      "label": "Requests Proxy URL",
      "env_variable": "REQUESTS_PROXY_URL",
      "placeholder": "http://proxy:8080",
      "hint": "Optional proxy URL for requests."
    },
    {
      "type": "text",
      "label": "Playwright CDP URL",
      "env_variable": "PLAYWRIGHT_CDP_URL",
      "default": "ws://browserless:3000?blockAds=true&stealth=true",
      "placeholder": "ws://browserless:3000?blockAds=true&stealth=true",
      "hint": "Playwright Chrome DevTools Protocol URL for browserless integration."
    },
    {
      "type": "text",
      "label": "FlareSolverr URL",
      "env_variable": "FLARESOLVERR_URL",
      "default": "http://flaresolverr:8191/v1",
      "placeholder": "http://flaresolverr:8191/v1",
      "hint": "FlareSolverr service URL for Cloudflare bypass."
    },
    {
      "type": "text",
      "label": "TMDB API Key",
      "env_variable": "TMDB_API_KEY",
      "placeholder": "your_tmdb_api_key",
      "hint": "TMDB API key for metadata fetching."
    },
    {
      "type": "boolean",
      "label": "Enable Prowlarr Scraping",
      "env_variable": "IS_SCRAP_FROM_PROWLARR",
      "default": true,
      "hint": "Enable/disable Prowlarr scraping."
    },
    {
      "type": "text",
      "label": "Prowlarr URL",
      "env_variable": "PROWLARR_URL",
      "default": "http://prowlarr:9696",
      "placeholder": "http://prowlarr:9696",
      "hint": "Prowlarr service URL."
    },
    {
      "type": "password",
      "label": "Prowlarr API Key",
      "env_variable": "PROWLARR_API_KEY",
      "hint": "Prowlarr API key for authentication."
